 <header>
   <div class="container">
     <a href="/">
     <div class="logo">
       <h1>Launhstore</h1>
       <h2>Compra e Venda</h2>
     </div></a>
     <form action="/products/search" method="get" class="search">
      <input type="text" name="filter" placeholder="Buscar por produtos"
      {% if search.term %}
        val={{search.term}}
      {% endif %}
      >
     </form>
     <nav class="primary">
       <ul>
         <li>
            <a href="/ads/create">
              <i class="material-icons">add</i>
              Novo Anuncio
            </a>
          </li>

          <li class="accounts">
            <a href="/accounts">
            <i class="material-icons">account_circle</i>
            Minha conta
            </a>
            {%if session.userId%}
            <ul class=submenu>
              <li><a href="/users">Minha conta</a></li>
              <li><a href="/buying">Meus pedidos</a></li>
              <li><a href="/selling">Minhas vendas</a></li>
               <li>
                 <form action="/users/logout" method="post">
                  <button>Sair</button>
                  </form>
                 </li>
            </ul>
          </li>
       </ul>
       {%endif%}
     </nav>
   </div>
  </header>